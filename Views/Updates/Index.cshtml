@model IEnumerable<BTrack.Models.Update>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.accountId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.updateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.updateDetails)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.accountId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.updateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.updateDetails)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.bugId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.bugId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.bugId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
